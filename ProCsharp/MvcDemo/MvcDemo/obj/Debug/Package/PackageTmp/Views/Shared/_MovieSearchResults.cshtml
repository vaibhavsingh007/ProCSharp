@model IEnumerable<MvcDemo.Models.MovieDB>
<table id="searchResults">
    <tr>
        <th>
            Name
        </th>
        <th>
            Director and Date
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.Title, "Details", "Movies", new { id = item.ID }, null)
            </td>
            <td>
                @item.Director, @item.Date, @item.ID
            </td>
        </tr>
    }

</table>
